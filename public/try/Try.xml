            Sidebar
        </div>
        <div id="content">
            <div class="files">
                <!--<ui:ItemsView items="{files}">-->
                    <!--<js:Template name="item">-->

                    <!--</js:Template>-->
                <!--</ui:ItemsView>-->
            </div>
            <div class="result">
                <iframe cid="contentFrame" sandbox="allow-forms allow-pointer-lock allow-popups allow-same-origin allow-scripts"/>
            </div>

        </div>
    </div>

</try:TryClass>